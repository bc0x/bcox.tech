{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/next", "config": { "maxLambdaSize": "10mb" } },
    { "src": "./static/**", "use": "@now/static"}
  ],
  "routes": [{ "src": "^/blog/(?<id>[^/]*)", "dest": "/blogpost?id=$id" }],
  "name": "bcox.tech",
  "alias": ["bcox.tech", "brandoncox.tech"],
  "env": {
    "NODE_ENV": "production"
  }
}