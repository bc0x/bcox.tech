{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/next", "config": { "maxLambdaSize": "10mb" } }
  ],
  "routes": [{ "src": "^/blog/(?<title>[^/]*)", "dest": "/blogpost?title=$title" }],
  "name": "bcox.tech",
  "alias": ["bcox.tech", "brandoncox.tech"],
  "env": {
    "NODE_ENV": "production"
  }
}